programa
{
	
	funcao inicio()
	{
		cadeia info[3][3] = {{"joao","são paulo","(11) 9999-5241"},{"maria","ribeirão preto","(16) 9999-9506"},{"ana","manaus","(92) 9999-8574"}}
		inteiro contador = 0

		
		faca{
			escreva("Nome: "+info[contador][0]+"\nCidade: "+info[contador][1]+"\nTelefone: "+info[contador][2]+"\n\n")
			contador++	
		}enquanto (contador<3)
	}
}
