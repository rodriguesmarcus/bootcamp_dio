programa
{
	
	funcao inicio()
	{
		inteiro contador = 0
		cadeia cesta[][]={{"pera", "100"},{"jaca","200"},{"maçã","900"},{"uva","89"}}


		faca{
			escreva("Produto: "+cesta[contador][0]+"\nQuantidade: " + cesta[contador][1]+"\n\n")
			contador++
		}enquanto(contador<=3)
	}
}
